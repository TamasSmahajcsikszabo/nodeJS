<h1>User data for <%= subscriber.name %></h1>

<table class="table">
  <tr>
    <th>Name</th>
    <td><%= subscriber.name %></td>
  </tr>
  <tr>
    <th>Email</th>
    <td><%=subscriber.email%></td>
  </tr>
  <tr>
    <th>Zip Code</th>
    <td><%= subscriber.zipCode%></td>
  </tr>
  <tr>
    <th>Password</th>
    <td><%= subscriber.password%></td>
  </tr>
</table>

  <a href="/subscribers">
    <span class="button">Show all subscribers</span>
  </a>
  <a href="/users">
    <span class="button">Show all users</span>
  </a>
