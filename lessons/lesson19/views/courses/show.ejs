<h1>Course data for course <%= course.title %></h1>

<table class="table">
  <tr>
    <th>Title</th>
    <td><%= course.title %></td>
  </tr>
  <tr>
    <th>Description</th>
    <td><%= course.description %></td>
  </tr>
  <tr>
    <th> Items </th>
    <td><%= course.items %></td>
  </tr>
  <tr>
    <th>Zip Code </th>
    <td><%= course.zipCode %></td>
  </tr>
</table>

  <a href="/courses">
    <span class="button">Show all courses</span>
  </a>
